#include <bits/stdc++.h>
using namespace std;

void tcase() {$1$3$5
}
int32_t main() {
	ios_base::sync_with_stdio(false);
	cin.tie(nullptr);

	int t = 1, cs = 1;
	cin >> t;

	while (t-- > 0) {
		cout << "Case " << cs++ << ": ";
		tcase();
	}
}
